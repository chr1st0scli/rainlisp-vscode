{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "RainLisp",
	"patterns": [
		{ "include": "#boolean" },
		{ "include": "#number" },
		{ "include": "#string" },
		{ "include": "#comment" },
		{ "include": "#keyword" },
		{ "include": "#primitive" },
		{ "include": "#library" }
	],
	"repository": {
		"boolean": {
			"name": "literal.boolean.rainlisp",
			"match": "((?<=[()\\s])|^)(true|false)((?=[;'\"()\\s])|$)"
		},
		"number": {
			"name": "literal.number.rainlisp",
			"match": "((?<=[()\\s])|^)((\\+|-)?[0-9]+(\\.[0-9]*)?)((?=[;'\"()\\s])|$)"
		},
		"string": {
			"name": "literal.string.rainlisp",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "string.escape.sequence.rainlisp",
					"match": "\\\\."
				}
			]
		},
		"comment": {
			"name": "comment.rainlisp",
			"begin": ";",
			"end": "\n"
		},
		"keyword": {
			"name": "keyword.rainlisp",
			"match": "((?<=[()\\s])|^)(quote|set!|define|if|cond|else|begin|lambda|and|or|let)((?=[;'\"()\\s])|$)"
		},
		"primitive": {
			"name": "primitive.rainlisp",
			"match": "((?<=[()\\s])|^)(\\+|-|\\*|\\/|%|<|<=|=|>|>=|add-days|add-hours|add-milliseconds|add-minutes|add-months|add-seconds|add-years|car|cdr|cons|datetime-to-string|day|days-diff|debug|display|error|eval|hour|hours-diff|index-of-string|list|make-date|make-datetime|millisecond|milliseconds-diff|minute|minutes-diff|month|newline|nil|not|now|null\\?|number-to-string|pair\\?|parse-datetime|parse-number|replace-string|round|second|seconds-diff|set-car!|set-cdr!|string-length|substring|to-local|to-lower|to-upper|to-utc|trace|utc-now|utc\\?|xor|year)((?=[;'\"()\\s])|$)"
		},
		"library": {
			"name": "library.rainlisp",
			"match": "((?<=[()\\s])|^)(append|filter|flatmap|fold-left|fold-right|length|map|reduce|reverse|cddr|cadr|caar|cdar|cdddr|caddr|caadr|caaar|cdaar|cddar|cdadr|cadar|cddddr|cadddr|caaddr|caaadr|caaaar|cdaaar|cddaar|cdddar|cdadar|cadadr|cdaddr|cddadr|cadaar|caadar|cdaadr|caddar)((?=[;'\"()\\s])|$)"
		}
	},
	"scopeName": "source.rainlisp"
}